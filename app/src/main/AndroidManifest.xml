<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.oesia.agarciao.helloworld" >

    <!--<uses-permission android:name="android.permission.CALL_PHONE"/>-->

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >


        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DestinoActivity"
            android:label="@string/title_activity_destino" >
        </activity>
        <activity
            android:name=".ResultadoActivity"
            android:label="@string/title_activity_resultado" >
        </activity>
        <activity
            android:name=".ReceiverActivity"
            android:label="@string/title_activity_receiver" >
        </activity>
        <activity
            android:name=".ImplicitActivity"
            android:label="@string/title_activity_implicit" >
            <intent-filter>
                <action android:name="com.oesia.agarciao.helloworld.IMPLICITA" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Implicita2Activity"
            android:label="@string/title_activity_implicita2" >
            <intent-filter>
                <action android:name="com.oesia.agarciao.helloworld.IMPLICITA" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>
